{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && javascript-obfuscator dist/assets --output dist/assets",
    "build:dev": "vite build --mode development",
    "build:debug": "vite build --mode development --sourcemap",
    "build:no-minify": "vite build --minify false",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist",
    "build:minimal": "vite build --config vite.config.minimal.ts --mode production",
    "build:ultra-minimal": "vite build --config vite.config.minimal.ts --mode production --minify terser",
    "build:gzip": "vite build --mode production && gzip -k dist/assets/js/*.js dist/assets/css/*.css",
    "build:size": "vite build --mode production --emptyOutDir",
    "build:fast": "vite build --mode production --minify false",
    "build:analyze-size": "npm run build:minimal && du -sh dist/assets/js/*.js | sort -hr",
    "build:compress": "npm run build:minimal && gzip -k dist/assets/js/*.js dist/assets/css/*.css && echo 'Compressed files created'",
    "build:size-report": "npm run build:ultra-minimal && echo 'Build completed. Size analysis:' && ls -lah dist/assets/js/ && echo 'Total size:' && du -sh dist/",
    "build:production": "npm run build:ultra-minimal",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:debug": "vite preview --mode development",
    "analyze": "npx vite-bundle-analyzer"
  },
